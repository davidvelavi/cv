@import "nib"
@import "stylus/color.styl"
@import "stylus/pagina.styl"

*
	padding 0
	margin 0
	box-sizing border-box
	font-size 16px	
body
	background-color azulOscuro
	font-family: 'Noto Sans', sans-serif;
.Wrapper
	padding 20px 
	max-width 1500px
	margin 0 auto
.WrapperPagina
	margin-top 20px
	@media screen and (min-width:920px)
		display flex
		justify-content space-between